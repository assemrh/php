@model legarage.Models.ServicesModel

<div class="modal-header">
    <h5 class="modal-title" id="exampleModalLabel">@Resources.CP_Services.Edit</h5>
    <button type="button" class="close" data-dismiss="modal" aria-label="Close">
        <span aria-hidden="true">&times;</span>
    </button>
</div>
<div class="modal-body">
    <form id="EditForm">
        <input hidden type="text" id="country_id" name="id" value="@Model.ID" />
        <div class="form-group">
            <label for="country"><span style="color:red;">*</span>@Resources.CP_Services.Service</label>
            <input type="text" class="form-control" id="name" value="@Model.Name" name="name" placeholder="@Resources.CP_Services.EnterService">
        </div>
    </form>
</div>
<div class="modal-footer">
    <button type="button" class="btn btn-secondary" data-dismiss="modal">@Resources.CP_Services.Close</button>
    <button type="button" class="btn btn-primary" onclick="Update('@Model.URL.Edit','@Model.URL.Refresh')">@Resources.CP_Services.Edit</button>
</div>