@model legarage.Models.VehicleTypesModel

<div class="modal-header">
    <h5 class="modal-title">@Resources.CP_VehicleTypes.Details</h5>
    <button type="button" class="close" data-dismiss="modal" aria-label="Close">
        <span aria-hidden="true">&times;</span>
    </button>
</div>
<div class="modal-body">
    <input hidden type="text" value="@Model.ID" />
    <div class="form-group">
        <label for="country">@Resources.CP_VehicleTypes.VehicleType</label>
        <input disabled type="text" class="form-control" value="@Model.Name">
    </div>
</div>
<div class="modal-footer">
    <button type="button" class="btn btn-secondary" data-dismiss="modal">@Resources.CP_VehicleTypes.Close</button>
</div>