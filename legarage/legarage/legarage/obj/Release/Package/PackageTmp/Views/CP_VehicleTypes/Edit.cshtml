@model legarage.Models.VehicleTypesModel

<div class="modal-header">
    <h5 class="modal-title" id="exampleModalLabel">@Resources.CP_VehicleTypes.Edit</h5>
    <button type="button" class="close" data-dismiss="modal" aria-label="Close">
        <span aria-hidden="true">&times;</span>
    </button>
</div>
<div class="modal-body">
    <form id="EditForm">
        <input hidden type="text" id="vehicle_id" name="vehicle_id" value="@Model.ID" />
        <div class="form-group">
            <label for="type_name"><span style="color:red;">*</span>@Resources.CP_VehicleTypes.VehicleType</label>
            <input type="text" class="form-control" id="type_name" value="@Model.Name" name="type_name" placeholder="@Resources.CP_VehicleTypes.EnterVehicleType">
        </div>
    </form>
</div>
<div class="modal-footer">
    <button type="button" class="btn btn-secondary" data-dismiss="modal">@Resources.CP_VehicleTypes.Close</button>
    <button type="button" class="btn btn-primary" onclick="Update('@Model.URL.Edit','@Model.URL.Refresh')">@Resources.CP_VehicleTypes.Edit</button>
</div>
